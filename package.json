{
  "name": "potential-duplicates-bot",
  "version": "1.0.0",
  "description": "A Probot app",
  "author": "RussKie <russkie@gmail.com> (https://github.com/RussKie/potential-duplicates-bot)",
  "license": "ISC",
  "repository": "https://github.com/potential-duplicates-bot.git",
  "homepage": "https://github.com/potential-duplicates-bot",
  "bugs": "https://github.com/potential-duplicates-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./src/index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "joi": "^13.1.2",
    "mustache": "2.3.0",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "./src"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
